{
  "name": "terminal",
  "title": "Atom Integrated Terminal",
  "main": "./src/terminal",
  "repository": "https://github.com/atom-ide-community/terminal",
  "bugs": {
    "url": "https://github.com/atom-ide-community/terminal/issues/new/choose"
  },
  "version": "0.0.0",
  "description": "The xterm based terminal integrated into Atom",
  "keywords": [
    "terminal",
    "xterm",
    "term",
    "console",
    "shell",
    "emulator",
    "pty",
    "tty",
    "comspec",
    "command-line",
    "bash",
    "sh",
    "powershell",
    "cmd"
  ],
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "atomTestRunner": "./spec/custom-runner",
  "license": "MIT",
  "engines": {
    "atom": ">=1.50.0 <2.0.0"
  },
  "providedServices": {
    "platformioIDETerminal": {
      "description": "Run commands and open terminals.",
      "versions": {
        "1.1.0": "providePlatformIOIDEService"
      }
    },
    "terminal": {
      "description": "Change the terminal.",
      "versions": {
        "1.0.0": "provideTerminalService"
      }
    }
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "uuid": "^8.3.0",
    "whatwg-url": "^8.1.0",
    "xterm": "4.8.1",
    "xterm-addon-fit": "0.4.0",
    "xterm-addon-web-links": "0.4.0",
    "xterm-addon-webgl": "0.8.0"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^8.0.0",
    "atom-jasmine3-test-runner": "^5.0.5",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "semantic-release": "^17.1.1",
    "temp": "^0.9.1"
  },
  "scripts": {
    "lint": "eslint . --ext .json,.js",
    "test": "atom --test spec"
  },
  "deserializers": {
    "TerminalModel": "deserializeTerminalModel"
  }
}
